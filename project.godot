;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "FSM",
"language": "GDScript",
"path": "res://src/state/FSM.gd"
}, {
"base": "Node",
"class": "FSM_State",
"language": "GDScript",
"path": "res://src/state/FSM_State.gd"
}, {
"base": "Resource",
"class": "StateObject",
"language": "GDScript",
"path": "res://src/state/StateObject.gd"
} ]
_global_script_class_icons={
"FSM": "",
"FSM_State": "",
"StateObject": ""
}

[application]

config/name="Shade"
run/main_scene="res://src/screens/player_room/PlayerRoom.tscn"
config/icon="res://sprites/etc/Ghost.png"

[autoload]

Constants="*res://src/utils/Constants.gd"
Helpers="*res://src/utils/Helpers.gd"
TextBox="*res://src/ui/textbox/TextBox.tscn"
QuestionBox="*res://src/ui/question_box/QuestionBox.tscn"
Transition="*res://src/ui/transition/Transition.tscn"
PauseMenu="*res://src/ui/pause_menu/PauseMenu.tscn"

[display]

window/size/width=280
window/size/height=210
window/size/test_width=560
window/size/test_height=420
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor]

script_templates_search_path="res://src/templates"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

